algoritmo "Exercício 12"

var
Nome: caracter
Idade: inteiro
AnoAtual: inteiro
Ano: inteiro

inicio

AnoAtual <- 2020
Ano <- 365
 Escreval ("Digite seu nome pessoa:")
  leia (Nome)
 Escreval ("Digite sua idade:")
  leia (Idade)
  
Idade <- (Idade * ano)
Escreva ("O número de dias que você já viveu é" , Idade)
  



fimalgoritmo